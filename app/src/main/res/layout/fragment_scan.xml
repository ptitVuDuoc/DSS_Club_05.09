<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.ScanFragment">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fl_main_preview">
    </FrameLayout>
    <com.example.admin.dss_project.custom.view.scan.QrCodeFinderView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="@+id/fl_main_preview"
        app:layout_constraintTop_toTopOf="@+id/fl_main_preview"
        app:layout_constraintLeft_toLeftOf="@+id/fl_main_preview"
        app:layout_constraintRight_toRightOf="@+id/fl_main_preview"
        android:id="@+id/qr_code_view"/>

    <ImageView
        android:id="@+id/btn_enter_code"
        android:layout_width="@dimen/margin_padding_30"
        android:layout_height="@dimen/margin_padding_30"
        android:src="@drawable/entering_code"
        android:tint="@android:color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/margin_padding_50"
        android:layout_marginRight="@dimen/margin_padding_50"/>

</android.support.constraint.ConstraintLayout>